  /**
  * Slow Cross Fade
  * Gallery Type » Slideshow Full
  * From Will-Myers.com
  **/
  #sections .gallery-fullscreen-slideshow[data-transition="fade"] {
    --duration: 2s;
  
    figure,
    .gallery-fullscreen-slideshow-item-src{
      opacity:1;
    }
    .gallery-fullscreen-slideshow-item{
      visibility: visible;
    }
    figure:last-child {
      animation: fadeIn var(--duration, 2s) ease;
    }
  
    @keyframes fadeIn {
      from {
        opacity: 0;
      }
      to {
        opacity: 1;
      }
    }
  }
  
